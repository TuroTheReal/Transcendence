{
  "name": "transcendence-dev",
  "dockerComposeFile": "docker-compose.yml",
  "service": "dev",
  "workspaceFolder": "/app",
  "profiles": ["dev"],
  "customizations": {
    "vscode": {
      "extensions": [
        "esbenp.prettier-vscode",
        "dbaeumer.vscode-eslint"
      ]
    }
  },
  "mounts": [
    "source=${localWorkspaceFolder},target=/app,type=bind"
  ]
}
